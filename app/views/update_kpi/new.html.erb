<% provide(:title, "Update KPI") %>
<h1>Update KPI</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:update_kpi, url: update_kpi_path) do |f| %>
      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>

      <%= f.submit "Submit", class: "btn" %>
    <% end %>
  </div>
</div>
