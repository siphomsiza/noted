<p id="notice"><%= notice %></p>

<h1>Listing Funding Sources</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @funding_sources.each do |funding_source| %>
      <tr>
        <td><%= funding_source.name %></td>
        <td><%= link_to 'Show', funding_source %></td>
        <td><%= link_to 'Edit', edit_funding_source_path(funding_source) %></td>
        <td><%= link_to 'Destroy', funding_source, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Funding source', new_funding_source_path %>
