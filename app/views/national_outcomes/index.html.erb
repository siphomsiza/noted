<p id="notice"><%= notice %></p>

<h1>Listing National Outcomes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @national_outcomes.each do |national_outcome| %>
      <tr>
        <td><%= national_outcome.name %></td>
        <td><%= link_to 'Show', national_outcome %></td>
        <td><%= link_to 'Edit', edit_national_outcome_path(national_outcome) %></td>
        <td><%= link_to 'Destroy', national_outcome, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New National outcome', new_national_outcome_path %>
