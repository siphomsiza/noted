
<div class="table-responsive col-lg-12">

  <table class="table table-striped table-responsive display" id="update-revenuebysource-table">
      <tr>
        <th>Vote number</th>
        <th>Line item</th>
        <th>July</th>
        <th>August</th>
        <th>September</th>
        <th>October</th>
        <th>November</th>
        <th>December</th>
        <th>January</th>
        <th>February</th>
        <th>March</th>
        <th>April</th>
        <th>May</th>
        <th>June</th>
        <th colspan="1"></th>
      </tr>



      <% revenue_by_sources.each do |revenue_by_source| %>
        <tr>
          <td><%= revenue_by_source.vote_number %></td>
          <td><%= revenue_by_source.line_item %></td>
          <td><%= revenue_by_source.july %></td>
          <td><%= revenue_by_source.august %></td>
          <td><%= revenue_by_source.september %></td>
          <td><%= revenue_by_source.october %></td>
          <td><%= revenue_by_source.november %></td>
          <td><%= revenue_by_source.december %></td>
          <td><%= revenue_by_source.january %></td>
          <td><%= revenue_by_source.february %></td>
          <td><%= revenue_by_source.march %></td>
          <td><%= revenue_by_source.april %></td>
          <td><%= revenue_by_source.may %></td>
          <td><%= revenue_by_source.june %></td>
          <td><%= link_to edit_revenue_by_sources_path(revenue_by_source) ,remote: true,'data-target' => "#edit-revenue-by-sources-modal", 'data-toggle' => 'modal', class: "btn btn-mini", :title=>"Click to update Revenue by source" do %>
              <span class="glyphicon glyphicon-edit"></span>&nbsp;Edit
            <% end %>
              <div id="edit-revenue-by-sources-modal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="ModalLabel" aria-hidden="true">

              </div>
          </td>

        </tr>
      <% end %>

  </table>

</div>
