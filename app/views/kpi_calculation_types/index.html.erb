<p id="notice"><%= notice %></p>

<h1>Listing Kpi Calculation Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kpi_calculation_types.each do |kpi_calculation_type| %>
      <tr>
        <td><%= kpi_calculation_type.name %></td>
        <td><%= kpi_calculation_type.code %></td>
        <td><%= link_to 'Show', kpi_calculation_type %></td>
        <td><%= link_to 'Edit', edit_kpi_calculation_type_path(kpi_calculation_type) %></td>
        <td><%= link_to 'Destroy', kpi_calculation_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kpi calculation type', new_kpi_calculation_type_path %>
