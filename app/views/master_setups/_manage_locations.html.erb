<div class = "tab-pane active fade in" id = "manage_locations">
      <div class="col col-lg-12">
 <table class="table table-hover">
   <thead>
   <tr>
     <th>Ref</th>
     <th>Municipality</th>
     <th>Province</th>
     <th>Regions/Areas</th>
     <th>Actions</th>
   </tr>
 </thead>

 <tbody>
 <% @master_setups.each do |setup| %>
   <tr>
     <td><%=  setup.id %></td>
       <td><%=  setup.municipality %></td>
           <td><%=  setup.province %></td>
         <td>
       <ol>
         <% setup.regions.each do |region_name| %>
     <li><%= region_name.name %></li>
     <% end %>
   </ol>
   </td>
     <td><%= link_to edit_master_setup_path(setup),class: 'btn btn-mini btn-warning', :style=>"text-decoration: none;", remote: true,'data-target' => "#master-setup-modal", 'data-toggle' => 'modal', :title=>"Click to Master Setup" do %>
       <span class="glyphicon glyphicon-pencil"></span>&nbsp;Edit
     <% end %>
       <div id="master-setup-modal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="ModalLabel" aria-hidden="true">

       </div>
    &nbsp;<%= link_to setup,class: 'btn btn-mini btn-danger', method: :delete, data: { confirm: 'Are you sure?' }, :style=>"text-decoration: none;" do %>
       <span class="glyphicon glyphicon-remove-sign"></span>&nbsp;Delete
     <% end %>
     </td>
   </tr>
 <% end %>
 </tbody>
</table>
<div id="googleMap" style="height:200px;width:100%;"></div>
</div>
</div>
