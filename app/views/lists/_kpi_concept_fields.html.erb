<div class="row">
  <div class="col-lg-12">
    <fieldset>

      <%= f.label :name, "KPI Concept" %><br>
      <%= f.text_field :name, :class => "form-control",:autocomplete=>"off" %>
      <br/>
      <%= f.label :code %><br>
      <%= f.text_field :code, :class => "form-control",:autocomplete=>"off" %>
      <br/>
      <%= f.hidden_field :_destroy %>
      <%= link_to "Remove KPI Concept", '#', class: "btn remove_fields" %>
      <br/>
    </fieldset>
  </div>
</div>
