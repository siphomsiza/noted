<div class="row">
  <div class="col-lg-12">
    <fieldset>

      <%= f.label :name, "Ward Name:" %><br>
      <%= f.text_field :name, :class => "form-control",:autocomplete=>"off" %>
      <br/>
      <%= f.label :ward_no, "Ward No.:" %><br>
      <%= f.text_field :ward_no, :class => "form-control",:autocomplete=>"off" %>
      <br/>
      <%= f.label :mun_ref, "Mun Ref" %><br>
      <%= f.text_field :mun_ref, :class => "form-control",:autocomplete=>"off" %>
      <br/>
      <%= f.hidden_field :_destroy %>
      <%= link_to "Remove Ward", '#', class: "btn remove_fields" %>
      <br/>
    </fieldset>
  </div>
</div>
